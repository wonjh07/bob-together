{
  "name": "bob-together",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "types:generate": "supabase gen types typescript --project-id czrkmxjazndjqqwcjjua > src/types/database.types.ts",
    "types:generate:safe": "sh -c 'TMP_FILE=$(mktemp) && supabase gen types typescript --project-id czrkmxjazndjqqwcjjua > \"$TMP_FILE\" && mv \"$TMP_FILE\" src/types/database.types.ts'",
    "supabase:push:safe": "sh -c '[ -f supabase/.temp/project-ref ] || (echo \"Missing supabase link. Run: npx supabase link --project-ref czrkmxjazndjqqwcjjua\" && exit 1); supabase db push'",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:changed": "jest --onlyChanged",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.91.1",
    "@tanstack/react-query": "^5.71.10",
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/next-plugin": "^2.4.17",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.8.1",
    "supabase": "^1.8.1",
    "typescript": "^5"
  }
}
